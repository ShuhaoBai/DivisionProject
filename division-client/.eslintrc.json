{
  "extends": [
    "react-app",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    // "@typescript-eslint/explicit-function-return-type": [
    //   "error",
    //   {
    //     "allowTypedFunctionExpressions": true
    //   }
    // ]
  },
  "overrides": [
    {
      "files": [
        "**/*.ts?(x)"
      ],
      "extends": [
        "prettier/@typescript-eslint"
      ],
      "rules": {
        "@typescript-eslint/explicit-function-return-type": [
          "error",
          {
            "allowExpressions": true
          }
        ]
        // "@typescript-eslint/no-explicit-any": "error"
      }
    }
  ]
}